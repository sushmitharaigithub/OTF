<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="MGM Automation Suite" parallel="tests"
	thread-count="1">


	<test name="Chrome Test">
		<parameter name="browserValue" value="Chrome" />
		<classes>
			<class name="com.testRunners.Chrome_TestRunner" />
		</classes>
	</test>

	<listeners>
		<listener class-name="com.generateReports.GenerateReports" />
	</listeners>


</suite>
